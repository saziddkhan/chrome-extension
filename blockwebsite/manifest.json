{
  "manifest_version": 2,
  "name": "Procrastination Blocker",
  "version": "1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
 
  "browser_action": {
    "default_popup": "Popup.html",
    "default_title": "blocker"
  },
  "permissions": [
    "www.google.com/*"
   
  ]
}
